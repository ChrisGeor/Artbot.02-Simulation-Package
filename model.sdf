<?xml version="1.0"?>
  <sdf version="1.5">

    <model name="myartbot">
    <static>false</static>                                    <!--CHASSIS LINK-->
      <link name="chassis">
        <pose>0 0 0.05 0 0 0</pose>

        <collision name="collision">
          <geometry>
            <box>
              <size>1.5 0.5 0.15</size>
            </box>
          </geometry>
        </collision>

        <!-- <visual name="visual">
          <geometry>
            <box>
              <size>0.4 0.2 0.05</size>
            </box>
          </geometry>
        </visual> -->
      

	<visual name="visual">
		<pose>0 0 0 0 0 1.5708</pose>
		 <geometry>
		   <mesh>
		    <uri>model://artbot.02/meshes/art.dae</uri>
		   </mesh>
		 </geometry>
	</visual>

	</link> 
     
     <link name="wheelfl">                                  <!--FRONT LEFT WHEEL LINK-->
        <pose>0.1 0.115 0.05 0 1.5707 1.5707</pose>

        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

      <joint name="wheelfl_to_chassis" type="revolute">
        <pose>0 0 -0.03 0 0 0</pose>
        <child>wheelfl</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
       <!--   <limit>
            <lower>-3.141592</lower>
            <upper>3.141592</upper>
          </limit>
        </axis>
        <axis2>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-0.523598</lower>
            <upper>0.523598</upper>
          </limit>
        </axis2>  -->
	</axis>
      </joint>

      <link name="wheelfr">                                  <!--FRONT RIGHT WHEEL LINK-->
        <pose>0.1 -0.115 0.05 0 1.5707 1.5707</pose>

        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

      <joint name="wheelfr_to_chassis" type="revolute">
        <pose>0 0 0.03 0 0 0</pose>
        <child>wheelfr</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
      <!--    <limit>
            <lower>-3.141592</lower>
            <upper>3.141592</upper>
          </limit>
        </axis>
        <axis2>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-0.523598</lower>
            <upper>0.523598</upper>
          </limit>
        </axis2> -->
	</axis>
      </joint>

      <link name="wheelbl">                                  <!--BACK LEFT WHEEL LINK-->
        <pose>-0.1 0.115 0.05 0 1.5707 1.5707</pose>

        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

      <joint name="wheelbl_to_chassis" type="revolute">
        <pose>0 0 -0.03 0 0 0</pose>
        <child>wheelbl</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
      </joint>

      <link name="wheelbr">                                  <!--BACK RIGHT WHEEL LINK-->
        <pose>-0.1 -0.115 0.05 0 1.5707 1.5707</pose>

        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

      <joint name="wheelbr_to_chassis" type="revolute">
        <pose>0 0 0.03 0 0 0</pose>
        <child>wheelbr</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
      </joint>

      <link name="camera1">

      <sensor type="camera" name="my_sensor">
        <pose>1.1 0 0.05 0 0 0</pose>
        <camera>
          <horizontal_fov>2.1679</horizontal_fov>
          <image>
            <width>320</width>
            <height>240</height>
          </image>
          <clip>
            <near>0.1</near>
            <far>100</far>
          </clip>
        </camera>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        </plugin>
      </sensor>

      </link>

      <joint name="camera_to_chassis" type="fixed">
        <parent>chassis</parent>
        <child>camera1</child>
        <pose>0 0 0 0 0 0</pose>
      </joint>

      <link name="laser">
        <sensor type="ray" name="head_hokuyo_sensor">
          <pose>1 0 0.025 0 0 0</pose>
          <visualize>true</visualize>
          <update_rate>20</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>400</samples>
                <resolution>1</resolution>
                <min_angle>-1.570796</min_angle>
                <max_angle>1.570796</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.1</min>
              <max>10</max>
              <resolution>0.01</resolution>
            </range>
          </ray>
          <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
            <topicName>/scan</topicName>
        </plugin>
        </sensor>
      </link>
			
     <joint name="laser_to_chassis" type="fixed">
       <parent>chassis</parent>
       <child>laser</child>
       <pose> 0 0 0 0 0 0 </pose>
     </joint>

    <plugin name="object_controller" filename="libgazebo_ros_planar_move.so">
      	<commandTopic>cmd_vel</commandTopic>
      	<odometryTopic>odom</odometryTopic>
      	<odometryFrame>odom</odometryFrame>
      	<odometryRate>20.0</odometryRate>
      	<robotBaseFrame>chassis</robotBaseFrame>
   </plugin>

    </model>
  </sdf>
